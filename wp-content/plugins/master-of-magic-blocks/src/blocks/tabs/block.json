{
    "schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "master-of-magic-blocks/tabs",
    "title": "Tabs",
    "category": "master-of-magic-blocks",
    "icon": "slides",
    "description": "A container block for multiple tabs.",
    "version": "1.0.0",
    "allowedBlocks": [
        "master-of-magic-blocks/tab"
    ],
    "supports": {
        "interactivity": true,
        "align": true
    },
    "attributes": {
        "activeTabId": {
            "type": "string",
            "default": ""
        }
    },
    "providesContext": {
        "master-of-magic-blocks/activeTabId": "activeTabId"
    },
    "textdomain": "master-of-magic-blocks",
    "editorScript": "file:./index.js",
    "style": "file:./style-index.css",
    "render": "file:./render.php",
    "viewScriptModule": "file:./view.js"
}
